int wepidentif=0;
int togglescript=1;
int scriptactivation=0;

script "s797" enter
{
	GiveInventory("MakeVisible",1);	//this line alone was before ENTER script 942. Whatever it does, now it adds it here.
	if(CheckInventory("LimitScriptCheck"))
	{
		if(CheckInventory("LimitScriptActive"))
		{
			togglescript=1;
		}
		else
		{
			togglescript=0;
		}
		terminate;
	}
	else
	{
		togglescript=1;
		GiveInventory("LimitScriptCheck",1);
		GiveInventory("LimitScriptActive",1);
	}
}

script "s798" (int slotmsg)		//slot x full
{
	hudmessage(s:"Slot ",
		   d:slotmsg,
		   s:" full";
	HUDMSG_FADEOUT,100,3,0.5,0.8,2.0,0.5);
}

