script "s800" (void)		//this one, well, is another part of the dropping script
{
	GiveInventory("WeaponSoundStopper",1);
	if((CheckWeapon("AEoDHandGrenade"))
	|| (CheckWeapon("AEoDStickyMine"))
	|| (CheckWeapon("AEoDNecromanticBoneStaff"))
	|| (CheckWeapon("AEoDTNTBunny"))
	|| (CheckWeapon("AEoDTNT"))
	|| (CheckWeapon("AEoDRemoteDetonator"))
	|| (CheckWeapon("AEoDProxDetonator"))
	|| (CheckWeapon("AEoDGlyph"))
	|| (CheckWeapon("AEoDJackBomb"))
	|| (CheckWeapon("AEoDPortalGun"))
	|| (CheckWeapon("AEoDLemonNade")))
